{
  "name": "coda-language-support",
  "displayName": "Coda Language Support",
  "description": "Language support for Coda programming language",
  "publisher": "SujalVivekChoudhari",
  "icon": "./images/extention-icon.png",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "coda",
      "aliases": ["Coda", "coda"],
      "extensions": [".coda"],
      "configuration": "./language-configuration.json",
      "icon": {
        "dark": "./images/file-icon.png",
        "light": "./images/file-icon.png"
      }
    }],
    "grammars": [{
      "language": "coda",
      "scopeName": "source.coda",
      "path": "./syntaxes/coda.tmLanguage.json"
    }]
  },
  "repository": {
    "url": "https://github.com/SujalChoudhari/VSCodeCodaExtention",
    "type":"github"
  }
}
