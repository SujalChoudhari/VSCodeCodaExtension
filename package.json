{
  "name": "coda-language-support",
  "displayName": "Coda Language Support",
  "description": "Language support for Coda programming language",
  "publisher": "SujalVivekChoudhari",
  "icon": "./images/extention-icon.png",
  "keywords": [
    "coda",
    "coda language",
    "language",
    "extention",
    "sujal",
    "coda extention"
  ],
  "version": "0.3.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters"
  ],
  "contributes": {
    "configurationDefaults": {
      "json.format.enable": true
    },
    "languages": [
      {
        "id": "coda",
        "aliases": [
          "Coda",
          "coda"
        ],
        "extensions": [
          ".coda"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "dark": "./images/file-icon.png",
          "light": "./images/file-icon.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "coda",
        "scopeName": "source.coda",
        "path": "./syntaxes/coda.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "coda",
        "path": "./snippets/snippets.json"
      }
    ]
  },
  "repository": {
    "url": "https://github.com/SujalChoudhari/VSCodeCodaExtension",
    "type": "github"
  }
}